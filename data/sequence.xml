<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Setting not to update as a true, means think when we update the prefix as 'AP' from the UI & avoid the next execution
        start as an HP -->
    <data noupdate="1">
        
        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Appointment Reference</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>